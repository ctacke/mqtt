<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageTargetFallback>portable-net45+win8+wpa81+wp8</PackageTargetFallback>
        <DebugType>full</DebugType>
        <RootNamespace>ctacke.MQTT</RootNamespace>
        <Version>1.0.17253</Version>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>A lightweight, .NET Standard MQTT Client Library</Description>
        <Copyright>©2015-2017 Chris Tacke</Copyright>
        <PackageProjectUrl></PackageProjectUrl>
        <RepositoryUrl>https://github.com/ctacke/mqtt</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>MQTT</PackageTags>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="opennetcf-extensions-standard" Version="1.0.17160.0" />
    </ItemGroup>
    
    <ItemGroup>
      <Folder Include="Properties\" />
    </ItemGroup>
</Project>